## -*- sh -*-
## incomplete.test -- Test incomplete command handling

# Common definitions
if test -z "$srcdir"; then
    srcdir=echo "$0" | sed 's,[^/]*$,,'
    test "$srcdir" = "$0" && srcdir=.
    test -z "$srcdir" && srcdir=.
    test "${VERBOSE+set}" != set && VERBOSE=1
fi
echo 1..6
. $srcdir/defs.sh
NAME=dufaycolor_dt_captureone_export
pexec $RUNCOLORSCREEN stitch --ncols 2 "$srcdir/${NAME}_tile1.jpg" "$srcdir/${NAME}_tile2.jpg"  --out "out-${NAME}.csprj"  --report "out-${NAME}.txt" --no-cpfind --scanner-type=horisontally-moving-lens --min-screen-percentage=80 || exit 1
grep "Best match on offset -16,-10" "out-${NAME}.txt" || exit 1
echo 'ok 1 - stitch project construction'
pexec $RUNCOLORSCREEN stitch --ncols 2 "$srcdir/${NAME}_tile1.jpg" "$srcdir/${NAME}_tile2.jpg"  --out "out-${NAME}-nomesh.csprj"  --report "out-${NAME}.txt" --no-cpfind --scanner-type=horisontally-moving-lens --no-mesh --min-screen-percentage=80 || exit 1
grep "Best match on offset -16,-10" "out-${NAME}.txt" || exit 1
echo 'ok 2 - stitch project construction no mesh'
PARNAME=out-${NAME}
pexec $RUNCOLORSCREEN autodetect out-${NAME}.csprj ${PARNAME}.par --gamma 2.2 --min-screen-percentage=80 $* || exit 1
echo 'ok 3 - stitch project parameter autodetect'
pexec $RUNCOLORSCREEN autodetect out-${NAME}-nomesh.csprj ${PARNAME}-nomesh.par --min-screen-percentage=80 --gamma 2.2 $* || exit 1
echo 'ok 4 - stitch project parameter autodetect'
test_all_render_modes out-${NAME}.csprj ${PARNAME}
echo 'ok 5 - stitch project render'
test_all_render_modes out-${NAME}-nomesh.csprj ${PARNAME}
echo 'ok 6 - stitch project render'
